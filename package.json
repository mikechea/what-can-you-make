{
  "name": "what-can-you-make",
  "version": "0.0.1",
  "description": "a simple tool to see what can be made with what you have",
  "main": "dist/index.js",
  "repository": "git@github.com:hweeks/what-can-you-make.git",
  "author": "hweeks",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "webpack-dev-server",
    "build": "tsc",
    "lint": "eslint src/ server/ --ext .js,.jsx,.ts,.tsx",
    "dev:be": "node --inspect -r ts-node/register server/index.ts",
    "dev:be:brk": "node --inspect-brk -r ts-node/register server/index.ts",
    "test:src": "jest src/ --verbose false --config=src/jest.config.ts --forceExit",
    "test:server": "jest server/ --verbose false --config=server/jest.config.ts -i --forceExit",
    "test": "yarn test:src && yarn test:server"
  },
  "dependencies": {
    "@material-ui/core": "4.9.5",
    "axios": "^0.21.1",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "jest-fetch-mock": "^3.0.3",
    "lodash": "4.17.15",
    "mongoose": "5.9.5",
    "msw": "^0.31.0",
    "pluralize": "^8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.12.20",
    "react-is": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/plugin-transform-typescript": "7.12.1",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@testing-library/react": "^12.0.0",
    "@types/express": "4.17.3",
    "@types/jest": "^26.0.24",
    "@types/mongoose": "5.7.6",
    "@types/node": "13.9.1",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "awesome-typescript-loader": "5.2.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "8.0.6",
    "enzyme": "^3.11.0",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.19.0",
    "global": "^4.4.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "^27.0.6",
    "jest-dev-server": "^5.0.3",
    "jest-environment-jsdom": "^27.0.6",
    "jest-environment-jsdom-global": "^2.0.4",
    "lorem-ipsum": "2.0.3",
    "mongodb-memory-server": "6.3.3",
    "react-test-renderer": "^17.0.2",
    "source-map-loader": "0.2.4",
    "supertest": "^6.1.3",
    "ts-node": "8.6.2",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
